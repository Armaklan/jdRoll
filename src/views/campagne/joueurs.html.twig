	<table class="table table-striped table-hover campagneTable">
			<thead>
				<tr>
					<th colspan="2">Joueurs</th>
				</tr>
			</thead>
			<tbody>
				{% for participant in participants %}
				<tr id="inscription{{participant.id}}">
					<td>{{ participant.username }}</td>
					<td width="90px" style="text-align : right">
					{% if is_mj and participant.part_statut == 1 %}
						<a onClick="campagneConfig.deleteParticipant({{campagne.id}}, {{participant.id}});"
               title="DÃ©sinscrire" class="btn btn-danger-ssctrl iconeBtn"><i class="icon-remove"></i></a>
          {% else %}
            {% if is_mj %}
              <a href="campagneConfig.addParticipant({{campagne.id}}, {{participant.id}});" title="Valider l'inscription" class="btn btn-primary iconeBtn"><i class="icon-ok"></i></a>
              <a href="campagneConfig.deleteParticipant({{campagne.id}}, {{participant.id}});" title="Refuser l'inscription" class="btn btn-danger-ssctrl iconeBtn"><i class="icon-remove"></i></a>
            {% endif %}
					{% endif %}
					</td>
				<tr>
				{% endfor %}
			</tbody>
	</table>

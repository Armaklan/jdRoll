{
  "name": "jdroll",
  "version": "1.0.0",
  "description": "jdRoll\r ======",
  "dependencies": {
    "@open-wc/building-rollup": "^2.0.1",
    "lit": "^2.2.2",
    "ts-loader": "8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/web-components": "^6.4.22",
    "babel-loader": "^8.2.5",
    "bower": "^1.8.14",
    "less": "^4.1.2",
    "rollup": "^2.70.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "postinstall": "bower install",
    "build": "npm run build:web && npm run build:less",
    "build:web": "rollup -c rollup-index.config.js && rollup -c rollup-vendor.config.js",
    "build:less": "lessc --source-map web/theme.less dist/css/theme.min.css && lessc --source-map web/jdroll.angular.less dist/css/jdroll.angular.min.css",
    "composer": "docker-compose exec php composer install && docker-compose exec php composer dump-autoload -o",
    "sqldata": "docker-compose exec db sh /sql/load-data.sh",
    "env": "docker-compose up -d",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "readmeFilename": "README.md"
}
